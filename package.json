    "@ngrx/store": "12.0.0",
    "@ngrx/store-devtools": "12.0.0",
    "@nrwl/angular": "12.5.2",
    "moment": "^2.29.1",
    "ngx-markdown": "12.0.0",
    "rxjs": "7.0.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
