    "jest": "27.0.3",
    "jest-junit": "^12.0.0",
    "jest-preset-angular": "9.0.3",
    "prettier": "2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "27.0.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "4.2.4"
  }
